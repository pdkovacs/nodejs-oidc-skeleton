{
	"compilerOptions": {
		"outDir": "./build/",
		"sourceMap": true,
		"strictNullChecks": true,
		"noImplicitAny": true,
		"esModuleInterop": true,
		"module": "Node16",
		"target": "ES2022",
		"moduleResolution": "Node16",
		"lib": [
			"es2022"
		],
		"allowJs": true
	},
	"include": [
		"./src/**/*"
	]
}
